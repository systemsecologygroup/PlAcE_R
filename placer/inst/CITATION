citHeader("To cite placer in publications use:")

citEntry(entry = "Article",
  title        = "Defining confidence intervals and sample size to estimate the prevalence of plastic debris in seabird nests",
  author       = personList(as.person("Tavares, D.C."), as.person("Moura, J.F."), as.person("Acevedo-Trejos, E."),
                            as.person("Crawford, M."), as.person("Makhado, A."), as.person("Lavers, J. L."),
                            as.person("Witteveenm, M."), as.person("Ryan, P."), as.person("Merico, A.") ),
  journal      = "MEE",
  year         = "Submitted",
  volume       = "",
  number       = "",
  pages        = "",
  url          = "",

  textVersion  =
  paste("Tavares, D.C., et al. (Submitted)",
        "Defining confidence intervals and",
        "sample size to estimate the prevalence",
        "of plastic debris in seabird nests",
        "")
)
